B4J=true@Version=9.90@End=Sub Process_GlobalsEnd SubSub InitFileGenerator    FileGenerator.Initialize    Log("FileGenerator initialized")End SubSub CreateDirectoryIdempotent(folderPath As String) As Boolean    Try        Dim folder As File        folder = File.DirApp & "/" & folderPath        If File.Exists(folder, "") = False Then            File.MakeDir(folder, "")            Log("Created directory: " & folder)        Else            Log("Directory already exists: " & folder)        End If        Return True    Catch        LogError("Error creating directory: " & folderPath, LastException)        Return False    End TryCatch End SubSub CreateFile(folderPath As String, fileName As String, content As String) As Boolean    Try        Dim filePath As String        filePath = File.DirApp & "/" & folderPath & "/" & fileName        CreateDirectoryIdempotent(folderPath)        Dim wr As TextWriter        wr.Initialize(filePath)        wr.WriteLine(content)        wr.Close        Log("Created file: " & filePath)        Return True    Catch        LogError("Error creating file: " & fileName, LastException)        Return False    End TryCatch End SubSub CreateMultipleFiles(basePath As String, files As Map) As Int    Dim successCount As Int = 0    Dim iterator As Iterator = files.Keys.Iterator    While iterator.HasNext        Dim fileName As String = iterator.Next        Dim content As String = files.Get(fileName)        If CreateFile(basePath, fileName, content) Then            successCount = successCount + 1        End If    Wend    Return successCountEnd SubSub WriteToFile(filePath As String, content As String) As Boolean    Try        Dim wr As TextWriter        wr.Initialize(filePath)        wr.WriteLine(content)        wr.Close        Log("Wrote to file: " & filePath)        Return True    Catch        LogError("Error writing to file: " & filePath, LastException)        Return False    End TryCatch End SubSub ReadFromFile(filePath As String) As String    Try        Dim reader As TextReader        reader.Initialize(File.DirApp, filePath)        Dim content As String = ""        Dim line As String        Do While reader.ReadLine <> ""            line = reader.ReadLine            content = content & line & CRLF        Loop        reader.Close        Return content    Catch        LogError("Error reading file: " & filePath, LastException)        Return ""    End TryCatch End SubSub FileGenerator_Initialize    'Initialization logicEnd Sub