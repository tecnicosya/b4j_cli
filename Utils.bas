B4J=true@Version=9.90@End=Sub Process_GlobalsEnd SubSub ValidateFieldDefinition(fieldDef As Map) As Boolean    If fieldDef = Null Then        Return False    End If    If fieldDef.ContainsKey("name") = False Then        Return False    End If    If fieldDef.ContainsKey("type") = False Then        Return False    End If    Dim name As String = fieldDef.Get("name")    Dim fieldType As String = fieldDef.Get("type")    If name.Length = 0 Or fieldType.Length = 0 Then        Return False    End If    Return TrueEnd SubSub IsValidIdentifier(text As String) As Boolean    If text = Null Or text.Length = 0 Then        Return False    End If    Dim ch As String    Dim i As Int    For i = 0 To text.Length - 1        ch = text.CharAt(i)        If i = 0 Then            If (ch >= "a" And ch <= "z") Or (ch >= "A" And ch <= "Z") Or ch = "_" Then                'Valid start            Else                Return False            End If        Else            If (ch >= "a" And ch <= "z") Or (ch >= "A" And ch <= "Z") Or (ch >= "0" And ch <= "9") Or ch = "_" Then                'Valid            Else                Return False            End If        End If    Next    Return TrueEnd SubSub SanitizeFileName(fileName As String) As String    Dim result As String = ""    Dim i As Int    For i = 0 To fileName.Length - 1        Dim ch As String = fileName.CharAt(i)        If (ch >= "a" And ch <= "z") Or (ch >= "A" And ch <= "Z") Or (ch >= "0" And ch <= "9") Or ch = "_" Or ch = "-" Or ch = "." Then            result = result & ch        End If    Next    Return resultEnd SubSub GetCurrentTimestamp As Long    Return DateTime.NowEnd SubSub FormatDate(timestamp As Long) As String    Return DateTime.GetDate(timestamp)End SubSub FormatDateTime(timestamp As Long) As String    Dim date As String = DateTime.GetDate(timestamp)    Dim time As String = DateTime.GetTime(timestamp)    Return date & " " & timeEnd SubSub StringToList(text As String, delimiter As String) As List    Dim list As List    list.Initialize    Dim parts() As String    parts = text.Split(delimiter)    Dim i As Int    For i = 0 To parts.Length - 1        list.Add(parts(i))    Next    Return listEnd SubSub ListToString(list As List, delimiter As String) As String    Dim result As String = ""    Dim i As Int    For i = 0 To list.Size - 1        If i > 0 Then            result = result & delimiter        End If        result = result & list.Get(i)    Next    Return resultEnd SubSub TrimQuotes(text As String) As String    If text.StartsWith("""") And text.EndsWith("""") Then        Return text.SubString(1, text.Length - 1)    End If    Return textEnd SubSub Utils_InitializeEnd Sub